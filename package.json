{
  "name": "aida",
  "version": "0.4.1",
  "type": "module",
  "engines": {
    "node": "22.19.0"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "pdfmake": "^0.2.20",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.1",
    "@types/pdfmake": "^0.2.12",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "mocha": "^11.7.2",
    "supertest": "^7.2.2",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "prepare": "tsc -p src/tsconfig.json",
    "dev": "npm run prepare && node --env-file=.env dist/server.js",
    "start": "npm run prepare && node dist/server.js",
    "test": "tsc -p test/tsconfig.json && mocha --enable-source-maps --reporter spec --bail --check-leaks dist-test/test/test.js --exit",
    "x-test": "mocha --reporter spec --bail --check-leaks test/test.ts"
  }
}
